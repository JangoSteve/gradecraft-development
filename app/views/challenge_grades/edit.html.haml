.small-12.panel.mainContent
  / Challenge Grades Navigation
  .show-for-large-up
    = content_nav_for @challenge do
      = render "teams/tabs"
      
  .hide-for-large-up{:style => "padding: 10px"}
    = mobile_nav_for @challenge

  .small-12.columns

    .hide-for-large-up
      %a.radius.button.tiny.small-12.medium-4.dropdown{:href => "#", :data => { :dropdown => "drop" } }
        Select
      %ul#drop.f-dropdown
        = render "teams/tabs"

    %h3= @title
    = render 'form'

  .clearfix